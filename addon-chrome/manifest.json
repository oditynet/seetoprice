{
  "manifest_version": 3,
  "name": "seetoprice",
  "version": "1.2.2.2",
  "permissions": [
    "contextMenus",
    "storage",
    "notifications",
    "alarms",
    "scripting",
    "tabs"
  ],
  "host_permissions": ["<all_urls>"],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [{
    "matches": ["<all_urls>"],
    "js": ["content.js"],
    "run_at": "document_end"
  }],
  "action": {
    "default_icon": "icons/icon48.png",
    "default_popup": "popup/popup.html"
  },
  "icons": {
    "48": "icons/icon48.png"
  },
  "web_accessible_resources": [{
    "resources": ["import.html", "import.js"],
    "matches": ["<all_urls>"]
  }]
}