{
  "manifest_version": 3,
  "name": "seetoprice-dev",
  "version": "1.2.1.5",
  "permissions": [
    "contextMenus",
    "storage",
    "notifications",
    "alarms",
    "scripting"
  ],
  "host_permissions": ["<all_urls>"],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [{
    "matches": ["<all_urls>"],
    "js": ["content.js"],
    "run_at": "document_end"
  }],
  "action": {
    "default_icon": "icons/icon48.png",
    "default_popup": "popup/popup.html"
  },
  "icons": {
    "48": "icons/icon48.png"
  }
}