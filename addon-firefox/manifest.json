{
  "manifest_version": 2,
  "name": "seetoprice",
  "version": "1.2.1.7",
  "permissions": [
    "contextMenus",
    "storage",
    "notifications",
    "alarms",
    "tabs",
    "<all_urls>",
    "webRequest",
    "webRequestBlocking"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_end"
    }
  ],
  "browser_action": {
    "default_icon": "icons/icon48.png",
    "default_popup": "popup/popup.html"
  },
  "icons": {
    "48": "icons/icon48.png"
  },
  "license": "GPL-3.0",
  "browser_specific_settings": {
    "gecko": {
      "id": "oditynet@seetoprice"
    }
  }
}